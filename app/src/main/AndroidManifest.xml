<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.couch">

    <application
        android:allowBackup="true"
        android:icon="@drawable/couch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Couch">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713"/>
        <activity
            android:name=".Activity.Mode3Activity"
            android:exported="false"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Activity.Mode2Activity"
            android:exported="false"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Activity.Mode1Activity"
            android:exported="false"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Activity.RankActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Activity.AdActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Activity.StartActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".Activity.MainActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            />
    </application>

</manifest>